echo:Example PoSRIC Script%v Version 2%v!;
echo:;

echo:Using files example.rpf and tmp.rpf...;
use:example.rpf;
tmp:tmp.rpf;

echo:Testing formatting...;
format:;
giveUp:Test Failed!;

echo:Testing addFn...;
addFn:hello.txt;
giveUp:Test Failed!;

echo:Testing rmFile...;
rmFile:hello.txt;
giveUp:Test Failed!;

echo:Testing addFd...;
useName:example;
giveUp:Test Failed!;
addFd:example;
giveUp:Test Failed!;

echo:Testing addDir...;
addDir:fooDir;
useDir:fooDir;
giveUp:Test Failed!;
addFn:hello.txt;
giveUp:Test Failed!;

echo:Testing exitDir...;
exitDir:;
giveUp:Test Failed!;
addFn:world.txt;
giveUp:Test Failed!;

echo:Done!;
exit:;
